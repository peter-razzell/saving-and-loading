config_version=5

[application]

config/name="SavingAndLoading"
config/features=PackedStringArray("4.5", "C#", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

AudioManager="*res://Scripts/Audio/AudioManager.cs"
InventoryObjectManager="*res://Scripts/Interaction/InventoryObjects/InventoryObjectManager.cs"
ObjectSpawner="*res://Scripts/Game/ObjectSpawner.cs"

[display]

window/size/viewport_width=1164
window/size/viewport_height=680
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="SavingAndLoading"

[global_group]

Root="root node in the scene, holds the game script which holds other globally accessible scripts"
Persist="persistable object, must have 3 persist methods"
Level="all the things inside the level are deleted and reinstantiated by the game node. "
DoNotDestroyOnLoad=""
LevelEntrance="spawn point for level"
LevelExit=""
Player=""

